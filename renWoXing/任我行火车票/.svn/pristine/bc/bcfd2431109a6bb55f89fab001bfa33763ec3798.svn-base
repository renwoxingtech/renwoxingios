    }
    res.seatTypeArr = searTypeArr;
    
    UIView *superv = cell.zuowei1.superview;
    CGFloat w  = SWIDTH - 36-(arr.count+1)*3;
                        
    CGFloat ww = w/arr.count;
    for (UIView *v in superv.subviews) {
        v.hidden = YES;
    }
    NSInteger index = 1;
    for (NSString *price in arr) {
        UILabel *l = [superv viewWithTag:index];
        l.text = price;
       
        l.minimumScaleFactor = 0.7;
        if (index==1) {
            l.textAlignment = NSTextAlignmentLeft;
            
        }else if(index==arr.count){
            l.textAlignment = NSTextAlignmentRight;
            
        }else{
            
            l.textAlignment = NSTextAlignmentCenter;
        }
        l.left = 3+(ww+3)*(index-1);
        l.width = ww;
        [l sizeToFit];
        if (l.width>ww) {
            l.width = ww;

        }
      
        
        if (index>1) {
            UILabel *l2 = [superv viewWithTag:index-1];
            
            l.left = l2.right + 10;
        }
     
        if (index>1 && [arr[index-1-1] containsString:@":0张"]) {
            UILabel *l2 = [superv viewWithTag:index-1];
            
            l.left = l2.right + 15;
        }
        if ([price containsString:@":0张"]) {
            
